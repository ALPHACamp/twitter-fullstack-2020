<div id="main">
	{{#each profile.LikedTweets}}
		<div class="card mb-3 w-150">
			<div class="row no-gutters">
				<div class="col-md-4">
					<img src="{{this.User.avatar}}" class="rounded-circle" alt="user avatar">
				</div>
				<div class="col-md-8">
					<div class="card-body">
						<h5 class="card-title">
							<a >{{ this.User.name }}</a>
							<small class="text-muted">{{moment this.createdAt }}</small>
						</h5>
						<p class="card-text">
							{{this.description}}
						</p>
					</div>
				</div>
			</div>
		</div>
	{{/each}}
</div>