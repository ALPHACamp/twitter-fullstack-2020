<div class="row">
  {{> left-column}}
  <div class="center-section col-9 ">
    <div class="admin-tweets-list" style="height: 100vh;">
      <div class="title text-left font-weight-bold mt-1 py-3 pl-2"
        style="font-size: 18px;line-height: 26px;color: #1C1C1C;border: #E6ECF0 1px solid;">推文清單
      </div>
      {{#each tweets}}
      <div>
        {{this.User.account}}
        {{this.UserId}}
        {{this.description}}
      </div>
      {{/each}}
      <div class="card-footer text-muted follow-footer">
        顯示更多
      </div>
    </div>
  </div>