<div class="row">

  {{#each users}}
    <div class="card m-3" style="width: 250px;">
      <img class="card-img-top" src="https://ppt.cc/fAvApx" alt="Card image cap" style="width: 100%; height: 150px;">
      <img class="card-img mx-auto" src="https://ppt.cc/fAvApx" style="width: 100px; height: 100px; border-radius: 50%; margin-top: -60px; border: 4px solid white;">
      <div class="card-body">
        <h5 class="card-title text-center">{{this.name}}</h5>
        <div class="text-center"><small class="text-muted">{{this.id}}</small></div>
        <h5 class="card-title text-center">Like Dislike</h5>
        <h5 class="card-title text-center">Followed Following</h5>
      </div>
    </div>
  {{/each}}

</div>