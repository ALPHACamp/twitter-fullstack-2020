<div id="column1" style="float:left; width:20%; position: fixed; top:0%; left:5%">{{> adminNavigation}}</div>

<div class="d-flex justify-content-center">
  <div id="column2" class="mx-auto my-auto" style="float:left;width:50%;">
    <h3>使用者列表</h3>
    {{#each users}}
      <div>
        <div>
          <div>
            <img src='{{this.avatar}}' style= "width:50PX;height:50px; border-radius:100%;"></img>
            <img src='{{this.cover}}' style= "width:50PX;height:50px;"></img>
          </div>
          <span>{{this.name}}</span>
          @<span>{{this.account}}</span>
          <span>tweet{{this.tweetCount}}</span>
          <span>被追蹤{{this.followerCount}}</span>
          <span>追蹤{{this.followingCount}}</span>
          <span>like{{this.likeCount}}</span>
        </div>
      </div>
      <br />
    {{/each}}
  </div>
</div>