{{>left route='profile'}}
<div class="col-6">
  <div class="col container">
    <div class="content" style="border:1px solid #E6ECF0">
      {{>user-header}}
      {{>users-info}}
      {{>users-tab route='tweets'}}
      <div class="profile">
        {{#each tweets}}
          {{>tweet route='tweets'}}
        {{/each}}
      </div>
    </div>
  </div>
</div>
{{>right route='followers'}}
{{>pagination route='profile'}}