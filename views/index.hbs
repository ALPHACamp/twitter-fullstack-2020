{{!-- main feed container --}}
{{!-- check new or news --}}
{{!-- check form method --}}
{{!--
(.main-feed-container>(.news-feed-input-group>(h2.new-feed-title+(form.news-feed-form>(img.user-avatar+input.news-feed-input)+btn.news-feed-btn)))+(.news-feed-group>.twitter-item>(.user-img>img.user-avatar)+(.user-post>(p.user-name+span.user-name)+p.user-post+(i.comment-icon+span.commentnumber+i.like-icon+span.likenumber))))
--}}

{{!-- {{>left}} --}}
<div class="main-feed-container">
  <div class="news-feed-input-group">
    <h2 class="news-feed-title">首頁</h2>
    <form action="" class="news-feed-form"><img src="" alt="" class="user-avatar"><input type="text"
        class="news-feed-input"><button class="news-feed-btn"></button></form>
  </div>
  <div class="news-feed-group">
    {{!-- {{#each}} --}}
    <div class="twitter-item">
      <div class="user-img"><img src="" alt="" class="user-avatar"></div>
      <div class="user-post">
        <p class="user-name">Apple</p><span class="user-name">@apple</span>
        <p class="user-post">Lorem ipsum dolor sit amet, consectetur adipiscing elit. Pellentesque interdum rutrum
          sodales. Nullam mattis fermentum
          libero, non volutpat.</p>
        <i class="comment-icon"></i><span class="commentnumber"></span><i class="like-icon"></i><span
          class="likenumber"></span>
      </div>
    </div>
    {{!-- {{/each}} --}}
  </div>
</div>
{{!-- {{>right}} --}}