<div class="font-weight-bold h4">推文清單</div>
<br />
<br />

<table class="table">
  <tbody>
    {{#each tweets}}
    <tr>
      <td>
        <img class="img-responsive center-block" src="{{this.userAvatar}}" style="width: 50px;margin-bottom: 25px;">
      </td>
      <td>{{this.userName}}</td>
      <td>{{this.userAccount}}</td>
      <td>
        {{this.description}}
        <div>喜歡數:{{this.likeCount}}</div>
        <div>留言數:{{this.replayCount}}</div>
      </td>
      <td>{{moment this.createdAt}}</td>

    </tr>    
    {{/each}}
  </tbody>
</table>